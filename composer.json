{
    "name": "syluxso/requiem",
    "description": "HTTP requests that are easy to use and extend.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Syluxso\\Requiem\\": "src/"
        },
        "files": [
          "src/utils.php"
        ]
    },
    "authors": [
        {
            "name": "Syluxso",
            "email": "syluxso@gmail.com"
        }
    ],
    "repositories": [
      {
        "type": "vcs",
        "url": "https://github.com/Syluxso/Requiem"
      }
    ],
    "require": {
      "guzzlehttp/guzzle": "^7.9",
      "symfony/var-dumper": "^7.2.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^11.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
